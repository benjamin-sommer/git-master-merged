{
  "name": "git-master-merged",
  "version": "0.1.0-alpha.2",
  "description": "Check whether the latest commit of master has been merged into current branch for git projects.",
  "bin": {
    "git-master-merged": "./bin/git-master-merged.js"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix | snazzy",
    "precommit": "npm run lint"
  },
  "keywords": [
    "git",
    "merged",
    "master"
  ],
  "author": "elvinpeng <elvin.pjw@gmail.com>",
  "homepage": "https://github.com/elvinn/git-master-merged",
  "repository": {
    "type": "git",
    "url": "https://github.com/elvinn/git-master-merged.git"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^4.14.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  }
}
